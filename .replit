modules = ["web", "python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cargo", "libiconv", "rustc", "sshpass", "openssh"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev"

[[workflows.workflow]]
name = "dev"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33165
externalPort = 3000

[[ports]]
localPort = 35347
externalPort = 5173

[[ports]]
localPort = 36199
externalPort = 4200

[[ports]]
localPort = 37929
externalPort = 5000

[[ports]]
localPort = 38883
externalPort = 6000

[[ports]]
localPort = 41621
externalPort = 6800

[[ports]]
localPort = 42935
externalPort = 8008

[[ports]]
localPort = 43373
externalPort = 3003

[[ports]]
localPort = 45237
externalPort = 3002

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "server.py"]
